{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "dom",
      "es2015",
      "es2016"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": false,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types",
      "node_modules/phaser-ce/typescript/phaser.d.ts",
      "node_modules/phaser-ce/typescript/pixi.d.ts",
      "node_modules/phaser-ce/typescript/p2.d.ts",
      "node_modules/twitch-wrapper-ts/out/twitch-wrapper-ts.d.ts",
      "node_modules/twitch-wrapper-ts/out/apiTypes.d.ts",
      "node_modules/twitch-wrapper-ts/out/test.d.ts",
      "node_modules/twitch-wrapper-ts/out/utils.d.ts",
      "node_modules/twitch-wrapper-ts/out/api/endpoint.d.ts",
      "node_modules/twitch-wrapper-ts/out/twitch-wrapper-ts.d.ts",
      "node_modules/twitch-wrapper-ts/out/twitchError.d.ts",
      "node_modules/twitch-wrapper-ts/out/index.d.ts",
      "node_modules/twitch-wrapper-ts/out/parser.d.ts",
      "node_modules/twitch-wrapper-ts/out/looseObject.d.ts",
      "node_modules/twitch-wrapper-ts/out/requester.d.ts",
      "node_modules/twitch-wrapper-ts/esm/apiTypes.d.ts",
      "node_modules/twitch-wrapper-ts/esm/utils.d.ts",
      "node_modules/twitch-wrapper-ts/esm/twitch-wrapper-ts.d.ts",
      "node_modules/twitch-wrapper-ts/esm/twitchError.d.ts",
      "node_modules/twitch-wrapper-ts/esm/index.d.ts",
      "node_modules/twitch-wrapper-ts/esm/parser.d.ts",
      "node_modules/twitch-wrapper-ts/esm/looseObject.d.ts",
      "node_modules/twitch-wrapper-ts/esm/requester.d.ts"
    ],
    "outDir": "dist"
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "src/**/*.spec.ts",
    "src/**/__tests__/*.ts"
  ],
  "compileOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  }
}