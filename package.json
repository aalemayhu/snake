{
  "name": "phaser-typescript-webpack",
  "version": "1.0.0",
  "description": "A good place to get started with Phaser and TypeScript development",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.config.js -d --inline --progress --no-info --port 8000",
    "build": "rimraf ./dist && webpack --config webpack.prod.config.js --progress",
    "lint": "tslint src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "ava-ts --timeout=60s -w __tests__/*.test.ts",
    "parcel:maps": "parcel index.html && cp -r assets dist/ && cp -r src/Scripts dist",
    "parcel": "parcel build index.html --no-source-maps && cp -r {src/Scripts,assets} dist/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/numbofathma/phaser-typescript-webpack.git"
  },
  "engines": {
    "node": ">= 7.0.0"
  },
  "author": "Costin Mirica <hi@costinmirica.com> (http://www.costinmirica.com/)",
  "license": "MIT",
  "homepage": "https://github.com/numbofathma/phaser-typescript-webpack",
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.0",
    "ava": "^0.25.0",
    "ava-ts": "^0.25.2",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-typescript": "^7.0.0-alpha.19",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^4.5.4",
    "dotenv-webpack": "^1.5.7",
    "expose-loader": "^0.7.1",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "parcel-bundler": "^1.10.3",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "terser-webpack-plugin": "^1.1.0",
    "ts-loader": "^5.2.2",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "tslint-webpack-plugin": "^1.0.0",
    "typescript": "^3.1.3",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-obfuscator": "^0.18.0",
    "webpack-uglify-js-plugin": "^1.1.9"
