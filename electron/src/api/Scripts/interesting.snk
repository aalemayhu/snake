module.exports = function Next(views, sViews, body) {
    const treatMatch = views.find(v => { return v == "treat"});
    if (treatMatch) {
      return treatMatch.direction;
    }
    let v = views.map(e => {
        if (e.contains !== "player" && e.contains !== "wall") {
            return e;
        }
    });
    return v[~~(Math.random() * v.length)];
};
